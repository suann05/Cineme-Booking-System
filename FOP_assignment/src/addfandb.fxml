<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #FFF8F5;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="addfandbController">
   <children>
      <Button fx:id="backButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: transparent;" text="&lt; Back" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="foodTextField" layoutX="210.0" layoutY="155.0" promptText="Food Name">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <Label layoutX="222.0" layoutY="82.0" text="Food &amp; Beverage" textFill="#193a6f">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="desTextField" layoutX="211.0" layoutY="230.0" promptText="Description(2pcs/box)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="priceTextField" layoutX="211.0" layoutY="306.0" promptText="Price(RM)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <Button fx:id="addButton" layoutX="238.0" layoutY="399.0" mnemonicParsing="false" onAction="#addButton" style="-fx-background-color: #193A6F;" text="Add" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <Button fx:id="removeButton" layoutX="304.0" layoutY="399.0" mnemonicParsing="false" onAction="#removeButton" style="-fx-background-color: #193A6F;" text="Remove" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
