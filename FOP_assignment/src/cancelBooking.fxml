<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #FFF8F5;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="cancelBookingController">
   <children>
      <Label layoutX="232.0" layoutY="46.0" text="Cancel Booking" textFill="#193a6f">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: transparent;" text="&lt; Back" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="nameTextField" layoutX="210.0" layoutY="114.0" promptText="Movie Name">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="dateTextField" layoutX="211.0" layoutY="189.0" promptText="Date(1/1/2022)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="timeTextField" layoutX="211.0" layoutY="265.0" promptText="Time(12:00)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="seatTextField" layoutX="210.0" layoutY="342.0" promptText="Seat(A1)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <Button fx:id="submitButton" layoutX="271.0" layoutY="413.0" mnemonicParsing="false" onAction="#submitButton" style="-fx-background-color: #193A6F;" text="Submit" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
