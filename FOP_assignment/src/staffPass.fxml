<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #FFF8F5;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="staffPassController">
   <children>
      <Label layoutX="153.0" layoutY="135.0" text="Please enter the staff password" textFill="#193a6f">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="220.0" layoutY="232.0" promptText="Staff password" style="-fx-background-color: transparent; -fx-border-color: #193A6F; -fx-border-width: 2px;">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </PasswordField>
      <Button fx:id="submitButton" layoutX="273.0" layoutY="334.0" mnemonicParsing="false" onAction="#submitButton" style="-fx-background-color: #193A6F;" text="Submit" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="18.0" layoutY="12.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: transparent;" text="&lt; Back" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
