<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #FFF8F5;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="addPaymentController">
   <children>
      <Label layoutX="261.0" layoutY="76.0" text="Payment" textFill="#193a6f">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: transparent;" text="&lt; Back" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="studentTextField" layoutX="210.0" layoutY="134.0" promptText="Student Price(RM)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="classicTextField" layoutX="211.0" layoutY="209.0" promptText="Classic(RM)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="premiumTextField" layoutX="211.0" layoutY="285.0" promptText="Premium(RM)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
      <Button fx:id="submitButton" layoutX="272.0" layoutY="434.0" mnemonicParsing="false" onAction="#submitButton" style="-fx-background-color: #193A6F;" text="Submit" textFill="#f98125">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="dateTextField" layoutX="210.0" layoutY="361.0" promptText="Date updated(1/1/2022)">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
